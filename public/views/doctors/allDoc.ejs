<!DOCTYPE html>
<html lang="en">


	<h1>Doctor List</h1>
	
<table class="table table-condensed table-striped">
            <thead>
            <tr>
                <th>Name</th>
                <th>Area</th>
                <th>Specialty</th>
                <th>Address</th>
                <th>Contact</th>
            </tr>
            </thead>
            <tbody>
                <% for(var i=0; i<doctor.length; i++) { %>
    
            <tr>
                <td><%= doctor[i].name %></td>
                <td><%= doctor[i].area%></td>
                <td><%= doctor[i].description%></td>
                <td><%=doctor[i].address%></td>
                <td><%=doctor[i].contact%></td>
            </tr>
            <% } %>
            
            </tbody>
		</table>
<% console.log(doctor) %>
  
    
</html>